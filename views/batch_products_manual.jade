    <!-- 定制套餐与项目组合-->
    div.page.log-page.on.content.mark
      div.handtailor_title
        h1 #{company}健康体检
        h1.title_h1 定制的套餐与项目组合
        div
          dl
            dd #{combos_count}
              span 类
            dt 涵盖项目组合
          dl
            dd #{configurations.length}
              span 种
            dt 体检套餐
          <!-- 项目开始 -->
      mixin createPackage(idx, maxValue)
        ul.ullist.row
          div
            - for(var x = idx; x <= configurations.length; x++)
              if(x <= maxValue)
                li
                  div.sexdiv(sex=configurations[x-1].sex)
                  div= configurations[x-1].name
       
            <!--项目内容开始-->
        each p_c in product_combos
          dl.title.row.group
            dd
            dt(cont=p_c.name)= p_c.name
          each config in p_c.configurations
            dl.children.row
              dd
                div
                  - for(var x = idx; x <= config.packages.length; x++)
                    if(x <= maxValue)
                      p(class="#{config.packages[x-1].sex}")
                        if config.packages[x-1].combo_used
                          span ●
                        else if config.packages[x-1].not_choosen
                          span.disabled ⊘
                        else
                          span.test ○
              dt= config.name
        <!--项目内容结束-->
        <!--项目价格开始-->
        div.pricediv.row    
          dlv.price_title
            dd
              div
                p
                p
                p
                p
                p
                p
            dt 套餐单价（元 / 人）
          dl.price
            dd
              div
                - for(var x = idx; x <= configurations.length; x++)
                  if(x <= maxValue)
                    p= configurations[x-1].market_price 
            dt 套餐价值
          dl.price
            dd
              div
                - for(var x = idx; x <= configurations.length; x++)
                  if(x <= maxValue)
                    p= configurations[x-1].price
            dt <span>*</span>现场自费价
          dl.price
            dd
              div
                - for(var x = idx; x <= configurations.length; x++)
                  if(x <= maxValue)
                    p= configurations[x-1].discount_price
            dt 团体优惠价
           <!--项目价格结束-->

      mixin callbackcreate(num, cot)
        - for(var x = num; x <= configurations.length; x++)
          if(x % cot == 0)
            +createPackage(num, x)
            +callbackcreate(x+1, cot)
            x = 999
          else if(x == configurations.length)
            +createPackage(num, configurations.length)

      +callbackcreate(1, 6)
      <!--p.page_number 2-->
    <!--意义及明细--> 
    div.page.log-page.content
      div.gist_title
        h1 贵单位定制的体检套餐中
        h1.title_h1 项目的检查意义
          span 及
          涵盖项目明细
        div
          dl
            dd #{items_count}
              span 项
            dt 涵盖项目总数
      each p_c in product_combos
        div.gist_con_title.row.group
          h2(cont=p_c.name)= p_c.name
          p.ddp 检查意义
          p项目
          p项目
        each config in p_c.configurations
          div.gist_con.row
            dd
              if config.mean
                div= config.mean
              else
                div ——
              ul
                each item in config.items
