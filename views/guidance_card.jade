  body
    .guidance-cards-content-out
      .guidance-cards-content
        div.guidance-cards-content-name.nochange(id='name') #{name}
        div.guidance-cards-content-sex.nochange(id='sex') #{sex}
        div.guidance-cards-content-age.nochange(id='age') #{age}
        div.guidance-cards-content-month.nochange(id='month') #{check_date}
        div.guidance-cards-content-week.nochange(id='week') #{weekday}
        div.guidance-cards-content-time.nochange(id='time') #{check_time}
        div.guidance-cards-content-company.nochange(id='company') #{source}
        div.guidance-cards-content-company-title 公司
        div.guidance-cards-content-department.nochange(id='division') #{division}
        div.guidance-cards-content-department-title 部门
        div.guidance-cards-content-rooms(id='rooms')
          each room in rooms
            div.guidance-cards-content-room(status='#{room.status}')
              span.nochange #{room.door_name}
              span.door_number.nochange #{room.door_number}
              div
                each item in room.items
                  span.nochange #{item}
        div
          div.barcode(id='barcode') !{barcode}
        div.guidance-cards-content-note(id='note') #{note}
          
